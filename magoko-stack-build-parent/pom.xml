<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0
     https://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.deepinthink.magoko</groupId>
    <artifactId>magoko-stack-parent</artifactId>
    <version>0.0.1-SNAPSHOT</version>
    <relativePath>../pom.xml</relativePath>
  </parent>
  <artifactId>magoko-stack-build-parent</artifactId>
  <packaging>pom</packaging>
  <name>magoko-stack-build-parent</name>
  <description>MagOKO Stack :: Build Parent</description>
  <properties>
    <dep.spring-boot.version>2.3.10.RELEASE</dep.spring-boot.version>
    <dep.magoko-config.version>0.0.1-SNAPSHOT</dep.magoko-config.version>
    <dep.magoko-login.version>0.0.1-SNAPSHOT</dep.magoko-login.version>
    <dep.magoko-pay.version>0.0.1-SNAPSHOT</dep.magoko-pay.version>
    <dep.magoko-tinker.version>0.0.1-SNAPSHOT</dep.magoko-tinker.version>
    <dep.magoko-admin.version>0.0.1-SNAPSHOT</dep.magoko-admin.version>
    <dep.magoko-broker.version>0.0.1-SNAPSHOT</dep.magoko-broker.version>
    <parameter.license-header.location>
      ${basedir}/../magoko-stack-build-parent/src/license-header.txt
    </parameter.license-header.location>
  </properties>
  <dependencyManagement>
    <dependencies>
      <dependency>
        <groupId>org.deepinthink.magoko</groupId>
        <artifactId>magoko-stack-bom</artifactId>
        <version>${project.version}</version>
        <type>pom</type>
        <scope>import</scope>
      </dependency>
      <dependency>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-dependencies</artifactId>
        <version>${dep.spring-boot.version}</version>
        <type>pom</type>
        <scope>import</scope>
      </dependency>
      <dependency>
        <groupId>org.deepinthink.magoko</groupId>
        <artifactId>magoko-config-bom</artifactId>
        <version>${dep.magoko-config.version}</version>
        <type>pom</type>
        <scope>import</scope>
      </dependency>
      <dependency>
        <groupId>org.deepinthink.magoko</groupId>
        <artifactId>magoko-login-bom</artifactId>
        <version>${dep.magoko-login.version}</version>
        <type>pom</type>
        <scope>import</scope>
      </dependency>
      <dependency>
        <groupId>org.deepinthink.magoko</groupId>
        <artifactId>magoko-pay-bom</artifactId>
        <version>${dep.magoko-pay.version}</version>
        <type>pom</type>
        <scope>import</scope>
      </dependency>
      <dependency>
        <groupId>org.deepinthink.magoko</groupId>
        <artifactId>magoko-tinker-bom</artifactId>
        <version>${dep.magoko-tinker.version}</version>
        <type>pom</type>
        <scope>import</scope>
      </dependency>
      <dependency>
        <groupId>org.deepinthink.magoko</groupId>
        <artifactId>magoko-admin-bom</artifactId>
        <version>${dep.magoko-admin.version}</version>
        <type>pom</type>
        <scope>import</scope>
      </dependency>
      <dependency>
        <groupId>org.deepinthink.magoko</groupId>
        <artifactId>magoko-broker-bom</artifactId>
        <version>${dep.magoko-broker.version}</version>
        <type>pom</type>
        <scope>import</scope>
      </dependency>
    </dependencies>
  </dependencyManagement>
</project>
